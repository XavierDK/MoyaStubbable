# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Demo' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Demo  
  pod 'Moya'
  
  
  target 'DemoTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Moya'
  end

end

target 'MoyaStubbable' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MoyaStubbable
  
  pod 'Moya'

  target 'MoyaStubbableTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Moya'
  end

end
